<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="662.0" prefWidth="772.0" style="-fx-background-color: #FFE800;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.milton.concesionaria.controllers.VentaController">
   <children>
      <HBox layoutX="112.0" layoutY="60.0" prefHeight="88.0" prefWidth="548.0">
         <children>
            <Label alignment="CENTER" prefHeight="98.0" prefWidth="581.0" text="Venta Vehiculo:">
               <font>
                  <Font name="System Bold" size="35.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox layoutX="175.0" layoutY="170.0" prefHeight="281.0" prefWidth="422.0" spacing="7.0">
         <children>
            <Label text="Modelo:" />
            <TextField fx:id="settextmodel" prefHeight="31.0" prefWidth="574.0" />
            <Label text="Año:" />
            <TextField fx:id="setTextAÑO" />
            <Label text="Color:" />
            <TextField fx:id="setTextcolor" />
            <Label text="Serie de Motor" />
            <TextField fx:id="setTextSerie" />
            <Label text="Precio:" />
            <TextField fx:id="setTextPrecio" />
